<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>projectinfoservlet</servlet-name>
  	<servlet-class>com.sp.xmgl.servlet.ProjectInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.sp.xmgl.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProjectUp</servlet-name>
    <servlet-class>com.sp.xmgl.servlet.ProjectUp</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TaskManagerServlet</servlet-name>
    <servlet-class>com.sp.xmgl.servlet.TaskManagerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>projectplanservlet</servlet-name>
    <servlet-class>com.sp.xmgl.servlet.ProjectPlanServlet</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>addplanservlet</servlet-name>
    <servlet-class>com.sp.xmgl.servlet.Addplanservlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>projectinfoservlet</servlet-name>
  	<url-pattern>/servlet/projectinfoservlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>projectplanservlet</servlet-name>
  	<url-pattern>/servlet/projectplanservlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>addplanservlet</servlet-name>
  	<url-pattern>/servlet/addplanservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/loginservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProjectUp</servlet-name>
    <url-pattern>/servlet/projectUp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TaskManagerServlet</servlet-name>
    <url-pattern>/servlet/taskmanagerservlet</url-pattern>
  </servlet-mapping>

  <filter>
  	<filter-name>myfilter</filter-name>
  	<filter-class>com.sp.xmgl.common.filter.MyFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>myfilter</filter-name>
  	<url-pattern>*.jsp</url-pattern>
  </filter-mapping>
   <filter-mapping>
  	<filter-name>myfilter</filter-name>
  	<url-pattern>/servlet/*</url-pattern>
  </filter-mapping>
</web-app>
